#nav ul li:first-child>a{
	color: #fff;
}
#main{
	background: url(../images/index_background.png) no-repeat;
	background-position: center;
	position: relative;
	margin: 0 auto;
	z-index: 900;
}
.ie #main{
	width: 1175px !important;
	height: 472px !important;
}
#block{
	left: 0;
	width: 76px;
}
#boy{
	background: url(../images/boy.png) no-repeat;
	background-repeat: no-repeat;
	position: relative;
}
.ie #boy{
	left: 770px !important;
	top: 80px !important;
}
#boy a{
	position: absolute;
	display: block;
}
