#top{
    float: right;
    margin-top: 80px;
    margin-right: 90px;
    cursor: pointer;
}
#top:hover{
    animation:myaction 1s infinite;
    -webkit-animation: myaction 1s infinite;
    animation-direction:alternate;
    -webkit-animation-direction:alternate;
}
@keyframes myaction{
    from {margin-top: 80px;}
    to {margin-top: 40px;}
}
@-webkit-keyframes  myaction{
    from {margin-top: 80px;}
    to {margin-top: 40px;}
}
